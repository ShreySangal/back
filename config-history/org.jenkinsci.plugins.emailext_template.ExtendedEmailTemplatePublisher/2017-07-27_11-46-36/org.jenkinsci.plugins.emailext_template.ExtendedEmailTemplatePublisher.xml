                                &lt;thead&gt;
                                    &lt;tr&gt;
                                        &lt;th colspan=&quot;3&quot;&gt;${coberturaResult.relativeSourcePath}&lt;/th&gt;
                                    &lt;/tr&gt;
                                &lt;/thead&gt;
                                ${coberturaResult.sourceFileContent}

                            &lt;/table&gt;
                        &lt;/div&gt;
                    &lt;/j:when&gt;
                    &lt;j:otherwise&gt;
                        &lt;p&gt;
