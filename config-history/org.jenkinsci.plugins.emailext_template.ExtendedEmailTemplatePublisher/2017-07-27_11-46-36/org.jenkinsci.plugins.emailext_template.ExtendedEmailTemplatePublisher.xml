                                            (${childResult})
                                        &lt;/td&gt;
                                    &lt;/j:when&gt;
                                    &lt;j:otherwise&gt;
                                        &lt;td data=&quot;101&quot;&gt;N/A&lt;/td&gt;
                                    &lt;/j:otherwise&gt;
                                &lt;/j:choose&gt;
                            &lt;/j:forEach&gt;
                        &lt;/tr&gt;
                    &lt;/j:forEach&gt;
                &lt;/table&gt;
            &lt;/j:forEach&gt;
  &lt;/j:if&gt;
&lt;BR/&gt;
&lt;/j:if&gt;


&lt;!-- CONSOLE OUTPUT --&gt;

&lt;j:getStatic var=&quot;resultFailure&quot; field=&quot;FAILURE&quot; className=&quot;hudson.model.Result&quot;/&gt;
