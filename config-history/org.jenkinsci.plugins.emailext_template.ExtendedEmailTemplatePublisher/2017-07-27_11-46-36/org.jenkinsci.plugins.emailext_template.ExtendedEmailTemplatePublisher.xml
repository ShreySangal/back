        &lt;j:if test=&quot;${artifacts!=null and artifacts.size()&amp;gt;0}&quot;&gt;
      &lt;TR&gt;
        &lt;TD&gt;
      	  &lt;j:forEach var=&quot;f&quot; items=&quot;${artifacts}&quot;&gt;
      	    &lt;li&gt;
      	      &lt;a href=&quot;${rooturl}${mvnbld.url}artifact/${f}&quot;&gt;${f}&lt;/a&gt;
      	    &lt;/li&gt;
      	  &lt;/j:forEach&gt;
      	&lt;/TD&gt;
      &lt;/TR&gt;
        &lt;/j:if&gt;
        &lt;/j:forEach&gt;
      &lt;/j:forEach&gt;
  &lt;/TABLE&gt;
&lt;BR/&gt;  
&lt;/j:if&gt;


&lt;!-- JUnit TEMPLATE --&gt;

