        &lt;/j:if&gt;
        &lt;/j:forEach&gt;
      &lt;/j:forEach&gt;
  &lt;/TABLE&gt;
&lt;BR/&gt;  
&lt;/j:if&gt;


&lt;!-- JUnit TEMPLATE --&gt;

&lt;j:set var=&quot;junitResultList&quot; value=&quot;${it.JUnitTestResult}&quot; /&gt;
&lt;j:if test=&quot;${junitResultList.isEmpty()!=true}&quot;&gt;
  &lt;TABLE width=&quot;100%&quot;&gt;
    &lt;TR&gt;&lt;TD class=&quot;bg1&quot; colspan=&quot;2&quot;&gt;&lt;B&gt;JUnit Tests&lt;/B&gt;&lt;/TD&gt;&lt;/TR&gt;
    &lt;j:forEach var=&quot;junitResult&quot; items=&quot;${it.JUnitTestResult}&quot;&gt;
