  <builders>
    <hudson.tasks.Shell>
      <command>echo $HostName $IP
#ssh test@ansibleserver sudo bash -x ./ansible_test/newserver.sh &quot;$HostName&quot; &quot;$IP&quot;
sudo bash -x /home/test/ansible_test/newserver.sh &quot;$HostName&quot; &quot;$IP&quot;
echo &quot;Hostname : &quot;$HostName
echo $IP
echo $VPCID
echo $Subnet
echo $AppSubnetID1
echo $AppSubnetID2
echo $DBSubnetID1
echo $DBSubnetID2
echo $ELBSubnetAZ1
echo $ELBSubnetAZ2
echo $InstanceID</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>#!/bin/bash
echo &quot;&quot;
echo &quot;Triggering AWS CLI to generate AMI ID : &quot;
cd /tmp
/bin/aws ec2 create-image --instance-id $InstanceID  --name &quot;IMIMDD&quot; --description &quot;AMIBBC&quot; &gt; /tmp/temp1
/bin/chmod 777 /tmp/temp1
/bin/sleep 600s
export AMIID=`/bin/cat /tmp/temp1`
#export AMIID=`/bin/cat /tmp/temp1 | /bin/grep -i ImageId | /bin/cut -d&apos;&quot;&apos; -f4`
echo &quot;&quot;
echo &quot; New AMI ID created : &quot; $AMIID
echo &quot;AMIID=$AMIID&quot; &gt; /tmp/variable.properties</command>
    </hudson.tasks.Shell>
    <EnvInjectBuilder plugin="envinject@2.1">
      <info>
        <propertiesFilePath>/tmp/variable.properties</propertiesFilePath>
      </info>
    </EnvInjectBuilder>
    <hudson.plugins.parameterizedtrigger.TriggerBuilder plugin="parameterized-trigger@2.34">
      <configs>
        <hudson.plugins.parameterizedtrigger.BlockableBuildTriggerConfig>
          <configs>
            <hudson.plugins.parameterizedtrigger.PredefinedBuildParameters>
              <properties>VPCID=$VPCID
AppSubnetID1=$AppSubnetID1
AppSubnetID2=$AppSubnetID2
DBSubnetID1=$DBSubnetID1
DBSubnetID2=$DBSubnetID2
ELBSubnetAZ1=$ELBSubnetAZ1
ELBSubnetAZ2=$ELBSubnetAZ2</properties>
            </hudson.plugins.parameterizedtrigger.PredefinedBuildParameters>
            <hudson.plugins.parameterizedtrigger.FileBuildParameters>
              <propertiesFile>/tmp/variable.properties</propertiesFile>
              <failTriggerOnMissing>false</failTriggerOnMissing>
              <useMatrixChild>false</useMatrixChild>
              <onlyExactRuns>false</onlyExactRuns>
            </hudson.plugins.parameterizedtrigger.FileBuildParameters>
          </configs>
          <projects>AppData</projects>
          <condition>ALWAYS</condition>
          <triggerWithNoParameters>false</triggerWithNoParameters>
          <triggerFromChildProjects>false</triggerFromChildProjects>
          <buildAllNodesWithLabel>false</buildAllNodesWithLabel>
        </hudson.plugins.parameterizedtrigger.BlockableBuildTriggerConfig>
      </configs>
    </hudson.plugins.parameterizedtrigger.TriggerBuilder>
    <hudson.tasks.Shell>
      <command>export AMIID=`cat /tmp/variable.properties`
echo &quot;&quot;
echo &quot; The new AMI ID created by AWS CLI : &quot; $AMIID
echo &quot;&quot;</command>
    </hudson.tasks.Shell>
